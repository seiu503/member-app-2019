{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/core": "^2.10.7",
    "@material-ui/core": "^4.0.2",
    "@material-ui/icons": "^4.11.2",
    "chokidar-cli": "^2.1.0",
    "classnames": "^2.2.6",
    "env-path": "^1.0.7",
    "immutability-helper": "^3.0.0",
    "material-table": "^1.69.2",
    "material-ui-dropzone": "^3.5.0",
    "moment": "^2.29.2",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-google-invisible-recaptcha": "^0.2.11",
    "react-localize-redux": "^3.5.3",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^5.0.0",
    "redux": "^4.0.1",
    "redux-api-middleware": "^3.2.1",
    "redux-form": "^8.3.7",
    "shortid": "^2.2.14",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.9.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "autoprefixer": "^10.0.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "canvas": "^2.6.0",
    "check-prop-types": "^1.1.2",
    "enzyme": "^3.9.0",
    "enzyme-to-json": "^3.3.5",
    "eslint-plugin-import": "^2.18.2",
    "form-data": "^2.4.0",
    "fs": "^0.0.1-security",
    "jest-canvas-mock": "^2.3.1",
    "jest-enzyme": "^7.0.2",
    "jest-fetch-mock": "^2.1.2",
    "jest-localstorage-mock": "^2.4.0",
    "local-ip-url": "^1.0.1",
    "nock": "^13.0.11",
    "node-sass-chokidar": "^1.4.0",
    "postcss-cli": "^8.0.0",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:dev": "env-path -p .env.development yarn build",
    "build:staging": "env-path -p .env.staging yarn build",
    "test": "react-scripts test --resetMocks=false",
    "test-cov": "DEBUG=nock.scope:localhost react-scripts test --coverage --no-cache --watchAll --coverageDirectory=output/coverage/jest",
    "eject": "react-scripts eject",
    "build-task:scss-compile": "node-sass-chokidar --source-map true src/styles/scss/ -o public/css",
    "build-task:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'src/styles/scss/' -c 'npm run sass:build'",
    "dev": "npm-run-all -p sass:*"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "resetMocks": false
    },
    "coverageReporters": [
      "html",
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/serviceWorker.js"
    ]
  },
  "homepage": "https://test.seiu503signup.org"
}
