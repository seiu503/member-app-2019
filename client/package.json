{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^6.1.9",
    "@mui/lab": "^6.0.0-beta.17",
    "@mui/material": "^6.1.9",
    "@mui/styles": "^6.1.9",
    "@mui/system": "^6.1.9",
    "@reduxjs/toolkit": "^1.9.5",
    "chokidar-cli": "^3.0.0",
    "classnames": "^2.5.1",
    "env-path": "^1.0.7",
    "history": "^5.3.0",
    "i18next": "^22.5.1",
    "i18next-browser-languagedetector": "^8.0.0",
    "immutability-helper": "^3.1.1",
    "moment": "^2.29.2",
    "node-fetch": "^2.6.7",
    "postcss": "^8.4.47",
    "query-string": "^6.8.1",
    "react": "18.3.1",
    "react-app-polyfill": "^3.0.0",
    "react-dom": "18.3.1",
    "react-i18next": "^12.3.1",
    "react-redux": "^8.1.3",
    "react-router-dom": "^7.1.1",
    "redux": "^4.2.1",
    "redux-api-middleware": "^3.2.1",
    "redux-form": "^8.3.8",
    "shortid": "^2.2.14",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@babel/plugin-transform-private-property-in-object": "^7.24.7",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.9.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.0.0",
    "babel-jest": "^29.5.0",
    "cross-fetch": "^4.0.0",
    "jest": "^29.7.0",
    "jest-config": "^29.5.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-localstorage-mock": "^2.4.0",
    "jest-mock-action-creators": "^1.2.1",
    "jest-transform-stub": "^2.0.0",
    "msw": "^2.4.11",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^8.0.0",
    "prop-types": "^15.8.1",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.81.0",
    "vite": "^5.2.13",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "build:dev": "env-path -p .env.development REACT_APP_ENV_TEXT=development vite build",
    "build:staging": "env-path -p .env.staging REACT_APP_ENV_TEXT=staging vite build",
    "test": "DEBUG_PRINT_LIMIT=100000 node --experimental-vm-modules node_modules/jest/bin/jest.js --env=jsdom --maxWorkers=50% --watchAll --workerIdleMemoryLimit=512MB",
    "test-cov": "DEBUG_PRINT_LIMIT=100000 jest --maxWorkers=50% --coverage --watchAll --env=jsdom --coverageDirectory=output/coverage/jest --workerIdleMemoryLimit=512MB",
    "build-task:scss-compile": "sass src/styles/scss/:public/css",
    "build-task:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'src/styles/scss/' -c 'npm run sass:build'",
    "dev": "vite"
  },
  "eslintConfig": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "html",
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "coverageProvider": "v8",
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "customExportConditions": [
        ""
      ]
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.jsx",
      "!src/mocks/handlers.js"
    ],
    "transform": {
      "\\.[jt]sx?$": [
        "babel-jest",
        {
          "rootMode": "upward"
        }
      ],
      ".+\\.(css|scss|png|jpg|svg)$": "jest-transform-stub"
    },
    "resetMocks": false,
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js",
      "<rootDir>/src/setupTests.jsx"
    ]
  },
  "homepage": "."
}
