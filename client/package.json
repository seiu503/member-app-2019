{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/core": "^2.13.1",
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.81",
    "@mui/material": "^5.11.3",
    "@mui/styles": "^5.11.2",
    "@mui/system": "^5.11.2",
    "@reduxjs/toolkit": "^1.9.5",
    "chokidar-cli": "^3.0.0",
    "classnames": "^2.2.6",
    "env-path": "^1.0.7",
    "immutability-helper": "^3.0.0",
    "moment": "^2.29.2",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-dropzone": "^14.2.3",
    "react-google-invisible-recaptcha": "^0.2.12",
    "react-localize-redux": "^3.5.3",
    "react-redux": "^8.0.1",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^5.0.0",
    "redux": "^4.2.1",
    "redux-api-middleware": "^3.2.1",
    "redux-form": "^8.3.8",
    "shortid": "^2.2.14",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/generator": "^7.17.9",
    "@babel/plugin-syntax-flow": "^7.16.7",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.9.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "^14.2.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.0",
    "autoprefixer": "^10.0.0",
    "babel-preset-env": "^1.7.0",
    "canvas": "^2.6.0",
    "check-prop-types": "^1.1.2",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^8.14.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-testing-library": "^5.10.3",
    "form-data": "^2.4.0",
    "fs": "^0.0.1-security",
    "jest": "29.3.1",
    "jest-canvas-mock": "^2.3.1",
    "jest-config": "^29.5.0",
    "jest-environment-jsdom": "29.3.1",
    "jest-enzyme": "^7.1.2",
    "jest-fetch-mock": "^2.1.2",
    "jest-localstorage-mock": "^2.4.0",
    "jest-mock-action-creators": "^1.2.1",
    "local-ip-url": "^1.0.1",
    "msw": "^1.2.1",
    "nock": "^13.0.11",
    "postcss-cli": "^8.0.0",
    "react-test-renderer": "17.0.2",
    "redux-mock-store": "^1.5.4",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.2",
    "sass": "^1.51.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "build:dev": "env-path -p .env.development yarn build",
    "build:staging": "env-path -p .env.staging yarn build",
    "test": "react-scripts test --resetMocks=false --detectOpenHandles",
    "test-cov": "DEBUG=nock.scope:localhost react-scripts test --coverage --no-cache --watchAll --coverageDirectory=output/coverage/jest --detectOpenHandles",
    "eject": "react-scripts eject",
    "build-task:scss-compile": "sass src/styles/scss/:public/css",
    "build-task:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'src/styles/scss/' -c 'npm run sass:build'",
    "dev": "npm-run-all -p sass:watch"
  },
  "eslintConfig": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      },
      "resetMocks": false,
      "testEnvironment": "<rootDir>/setupTests.js"
    },
    "coverageReporters": [
      "html",
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/store/actions"
    ]
  },
  "homepage": "."
}
