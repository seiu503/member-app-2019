{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.11.0",
    "@mui/lab": "^5.0.0-alpha.81",
    "@mui/material": "^5.11.3",
    "@mui/styles": "^5.11.2",
    "@mui/system": "^5.11.2",
    "@reduxjs/toolkit": "^1.9.5",
    "chokidar-cli": "^3.0.0",
    "classnames": "^2.2.6",
    "env-path": "^1.0.7",
    "history": "^5.3.0",
    "i18next": "^22.5.1",
    "i18next-browser-languagedetector": "^7.0.2",
    "immutability-helper": "^3.0.0",
    "moment": "^2.29.2",
    "node-fetch": "^2.6.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-google-invisible-recaptcha": "^0.2.12",
    "react-i18next": "^12.3.1",
    "react-redux": "^8.0.1",
    "react-router-dom": "^6.26.2",
    "react-scripts": "^5.0.0",
    "redux": "^4.2.1",
    "redux-api-middleware": "^3.2.1",
    "redux-form": "^8.3.8",
    "shortid": "^2.2.14",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.9.0",
    "@craco/craco": "^7.1.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "12.1.5",
    "@testing-library/user-event": "^14.2.0",
    "autoprefixer": "^10.0.0",
    "babel-jest": "^29.5.0",
    "check-prop-types": "^1.1.2",
    "jest": "^29.5.0",
    "jest-config": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^2.1.2",
    "jest-localstorage-mock": "^2.4.0",
    "jest-mock-action-creators": "^1.2.1",
    "msw": "^1.2.1",
    "postcss-cli": "^8.0.0",
    "redux-mock-store": "^1.5.4",
    "sass": "^1.51.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "build:dev": "env-path -p .env.development REACT_APP_ENV_TEXT=development craco build",
    "build:staging": "env-path -p .env.staging REACT_APP_ENV_TEXT=staging craco build",
    "test": "DEBUG_PRINT_LIMIT=100000 craco test --maxWorkers=50% --workerIdleMemoryLimit=512MB",
    "test-cov": "craco test --maxWorkers=25% --coverage --watchAll --coverageDirectory=output/coverage/jest --workerIdleMemoryLimit=512MB",
    "eject": "craco eject",
    "build-task:scss-compile": "sass src/styles/scss/:public/css",
    "build-task:autoprefixer": "postcss public/css/*.css --use autoprefixer -d public/css",
    "sass:build": "npm-run-all -p build-task:*",
    "sass:watch": "chokidar 'src/styles/scss/' -c 'npm run sass:build'",
    "dev": "npm-run-all -p sass:watch"
  },
  "eslintConfig": {},
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "html",
      "json",
      "lcov",
      "text",
      "clover"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/mocks/handlers.js"
    ],
    "transform": {
      "\\.[jt]sx?$": "babel-jest"
    },
    "resetMocks": false
  },
  "homepage": "."
}
